{
  "name": "ts-react-boilerplate",
  "version": "1.0.0",
  "description": "Typescript React Jest Boilerplate",
  "main": "index.js",
  "repository": "https://github.com/bruno-bert/ts-react-boilerplate.git",
  "author": "Bruno Bertoni de Paula",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "node index",
    "dev:base": "webpack serve --config webpack.config.js",
    "dev": "yarn run dev:base --open",
    "test": "jest --passWithNoTests --no-cache --runInBand",
    "test:watch": "yarn test --watch",
    "test:staged": "yarn test --findRelatedTests",
    "test:ci": "yarn test --coverage",
    "test:coveralls": "yarn test:ci && coveralls < coverage/lcov.info",
    "lint": "eslint",
    "postinstall": "husky install",
    "lint-staged": "lint-staged"
  },
  "devDependencies": {
    "@testing-library/react": "^11.2.7",
    "@types/axios": "^0.14.0",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.3.0",
    "@types/react": "^17.0.6",
    "@types/react-dom": "^17.0.5",
    "@types/react-router-dom": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "coveralls": "^3.1.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.5",
    "eslint": "^7.26.0",
    "eslint-config-standard-with-typescript": "11",
    "eslint-plugin-import": "^2.23.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-standard": "^5.0.0",
    "faker": "^5.5.3",
    "git-commit-msg-linter": "^3.1.0",
    "husky": "^6.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^11.0.0",
    "node-sass": "^6.0.0",
    "sass-loader": "^11.1.1",
    "style-loader": "^2.0.0",
    "ts-jest": "^26.5.6",
    "ts-loader": "^9.2.1",
    "typescript": "^4.2.4",
    "webpack": "^5.37.1",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0"
  }
}
